<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/DECORATION/1.7.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/DECORATION/1.7.0 https://maven.apache.org/xsd/decoration-1.7.0.xsd" combine.self="override">
  <bannerRight>
    <name>Techpert It Solutions</name>
    <src>./images/logos/TechpertLogo.jpg</src>
    <href>https://techpertitsolutions.com</href>
    <width>138</width>
    <height>125</height>
  </bannerRight>

  <skin>
 <groupId>lt.velykis.maven.skins</groupId>
    <artifactId>reflow-maven-skin</artifactId>
    <version>1.1.1</version>
  </skin>

  <publishDate position="bottom" format="yyyy-MM-dd HH:mm:ss z" />
  <version position="navigation-bottom" />

  <custom>
    <reflowSkin>
      <localResources>true</localResources>

      <highlightJs>true</highlightJs>

      <brand>
        <name>${organization.name}</name>
        <href>${organization.url}</href>
      </brand>
      <slogan>Intellegence Delivered Fast.</slogan>
      <titleTemplate>%2$s | %1$s</titleTemplate>

      <topNav>parent|modules|reports</topNav>
      <navbarInverse>true</navbarInverse>

      <breadcrumbs>true</breadcrumbs>
      <toc>false</toc>

      <pages>
        <source-repository>
          <highlightJs>false</highlightJs>
        </source-repository>
        <issue-tracking>
          <highlightJs>false</highlightJs>
        </issue-tracking>
        <integration>
          <highlightJs>false</highlightJs>
        </integration>
      </pages>

      <bottomNav maxSpan="9">
        <column>${organization.name}</column> <!-- see match site.xml body menu name <menu name="${organization.name}" inherit="top"> below -->
        <column>parent|modules</column>
        <column>reports</column>
      </bottomNav>

      <bottomDescription>
        <![CDATA[<div><a href="${project.scm.url}"><img src="./images/logos/githublogo.png" alt="github" width="116" height="125"></a></div>
                 Catch us on Github.
                 <div><a href="${organization.url}" title="${organization.name}" class="builtBy"><img class="builtBy" alt="${organization.name}" src="./images/logos/TechpertLogo.jpg" width="80" height="15"></a></div>]]>
      </bottomDescription>

      <skinAttribution>false</skinAttribution>
    </reflowSkin>
  </custom>

  <body>
    <head>
      <![CDATA[<link rel="shortcut icon" href="./images/logos/shadowrun.jpg" />]]>
    </head>

    <breadcrumbs>
      <item name="Home" href="project-info.html" />
    </breadcrumbs>

    <menu name="${organization.name}" inherit="top">
      <item name="Home" href="project-info.html" />
      <item name="Official Website" href="${organization.url}" />
      <item name="Github" href="${project.scm.url}" />
      <item name="FaceBook" href="https://business.facebook.com/techpertIT/?business_id=131115501106471" />
    </menu>

    <menu inherit="bottom" ref="parent" />
    <menu inherit="bottom" ref="modules" />
    <menu inherit="bottom" ref="reports" />

    <footer>
     <!--  <![CDATA[<hr /><p>Yo! <a href="">MTV</a> is on 24 hours a day. You should catch a few.</p>]]> -->
    </footer>
  </body>
</project>